{
  "name": "devscard",
  "description": "Template for creating a comprehensive virtual CV for developers.",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=14",
    "npm": ">=6",
    "yarn": "please-use-npm",
    "pnpm": "please-use-npm"
  },
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "generate-cv": "node scripts/generate-cv.cjs",
    "prettier:check": "prettier --check --ignore-path .gitignore .",
    "prettier:write": "prettier --write --ignore-path .gitignore .",
    "ts:check": "tsc --jsx preserve --skipLibCheck",
    "check": "concurrently npm:*:check"
  },
  "dependencies": {
    "@floating-ui/dom": "1.1.0",
    "iconify-icon": "1.0.2",
    "nanoid": "4.0.0"
  },
  "devDependencies": {
    "@astrojs/image": "0.12.1",
    "@astrojs/react": "1.2.2",
    "@astrojs/tailwind": "2.1.3",
    "astro": "1.9.2",
    "astro-compress": "1.1.27",
    "concurrently": "7.6.0",
    "iconify-icon-names": "1.1.0",
    "locales-ts": "1.0.0",
    "postcss": "8.4.21",
    "prettier": "2.8.2",
    "prettier-plugin-astro": "0.7.2",
    "prettier-plugin-tailwindcss": "0.2.1",
    "puppeteer": "19.5.2",
    "rollup-plugin-visualizer": "5.9.0",
    "tailwindcss": "3.2.4",
    "typescript": "4.9.4"
  }
}
